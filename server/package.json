    "dotenv": "^16.0.1",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "http-errors": "^1.6.3",
    "http-proxy-middleware": "^2.0.6",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10",
